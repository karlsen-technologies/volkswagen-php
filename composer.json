{
    "name": "karlsen-technologies/volkswagen-php",
    "description": "Unofficial PHP package for using the WeConnect API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Andre Karlsen",
            "email": "thomas@kartech.no"
        }
    ],
    "autoload": {
        "psr-4": {"KarlsenTechnologies\\Volkswagen\\": "src/"}
